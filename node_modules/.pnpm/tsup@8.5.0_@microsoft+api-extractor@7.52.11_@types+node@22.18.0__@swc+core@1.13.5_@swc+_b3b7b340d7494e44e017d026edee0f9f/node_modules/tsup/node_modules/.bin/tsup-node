#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/MAC/Documents/Code/onchainkit-react/node_modules/.pnpm/tsup@8.5.0_@microsoft+api-extractor@7.52.11_@types+node@22.18.0__@swc+core@1.13.5_@swc+_b3b7b340d7494e44e017d026edee0f9f/node_modules/tsup/dist/node_modules:/Users/MAC/Documents/Code/onchainkit-react/node_modules/.pnpm/tsup@8.5.0_@microsoft+api-extractor@7.52.11_@types+node@22.18.0__@swc+core@1.13.5_@swc+_b3b7b340d7494e44e017d026edee0f9f/node_modules/tsup/node_modules:/Users/MAC/Documents/Code/onchainkit-react/node_modules/.pnpm/tsup@8.5.0_@microsoft+api-extractor@7.52.11_@types+node@22.18.0__@swc+core@1.13.5_@swc+_b3b7b340d7494e44e017d026edee0f9f/node_modules:/Users/MAC/Documents/Code/onchainkit-react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/MAC/Documents/Code/onchainkit-react/node_modules/.pnpm/tsup@8.5.0_@microsoft+api-extractor@7.52.11_@types+node@22.18.0__@swc+core@1.13.5_@swc+_b3b7b340d7494e44e017d026edee0f9f/node_modules/tsup/dist/node_modules:/Users/MAC/Documents/Code/onchainkit-react/node_modules/.pnpm/tsup@8.5.0_@microsoft+api-extractor@7.52.11_@types+node@22.18.0__@swc+core@1.13.5_@swc+_b3b7b340d7494e44e017d026edee0f9f/node_modules/tsup/node_modules:/Users/MAC/Documents/Code/onchainkit-react/node_modules/.pnpm/tsup@8.5.0_@microsoft+api-extractor@7.52.11_@types+node@22.18.0__@swc+core@1.13.5_@swc+_b3b7b340d7494e44e017d026edee0f9f/node_modules:/Users/MAC/Documents/Code/onchainkit-react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli-node.js" "$@"
else
  exec node  "$basedir/../../dist/cli-node.js" "$@"
fi
